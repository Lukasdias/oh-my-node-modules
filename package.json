{
  "name": "oh-my-node-modules",
  "version": "1.0.1",
  "description": "Visualize, analyze, and clean up node_modules directories to reclaim disk space",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "onm": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "AGENTS.md"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && bun build ./src/cli.ts --outdir ./dist --target node",
    "dev": "bun --watch src/cli.ts",
    "start": "bun dist/cli.js",
    "test": "vitest",
    "test:run": "bun test --run",
    "test:watch": "bun test --watch",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "keywords": [
    "node_modules",
    "cleanup",
    "disk-space",
    "cli",
    "tui",
    "terminal",
    "visualization",
    "node",
    "npm",
    "yarn",
    "pnpm",
    "bun"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "commander": "^14.0.3",
    "picocolors": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "bunchee": "^6.2.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
